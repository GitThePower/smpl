    "esbuild": "^0.15.0",
    "jest": "^28.0.0",
    "ts-jest": "^28.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.0.0",
    "aws-sdk": "^2.0.0",
    "constructs": "^10.0.0",
