{
  "name": "smpl",
  "version": "0.1.0",
  "description": "Automated Crypto Currency Trading via APIs",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cdk": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cleanup": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./cleanUp.ps1",
